xOf(searchInput) !== -1;\n    });\n\n  return (\n    <>\n      <div className='menu'>\n        <Menu toggleProfile={toggleProfile} />\n      </div>\n      <div className='playlists-container' onScroll={handleScroll}>\n        <div className='upper-nav'>\n          <h1 className='heading'>Your Playlists</h1>\n        </div>\n        <div className='search-container'>\n          <BiSearch />\n          <input className='search-input' type='text' placeholder='Search playlists' value={searchInput} onChange={updateSearch} />\n        </div>\n        <Playlist playlistData={filteredPlaylist} />\n\n        {isEmpty || filteredPlaylist.length < 5 ? (\n          ''\n        ) : (\n          <div className='spinner-container'>\n            <Spinner size='30px' />\n          </div>\n        )}\n      </div>\n    </>\n  );\n};\n\nexport default Playlists;\n",["150","151"],"/home/hansen/workspace/spotify-app/client/src/components/Navbar.js",["152","153","154"],"/home/hansen/workspace/spotify-app/client/src/pages/Recent.js",["155","156","157","158","159","160"],"/home/hansen/workspace/spotify-app/client/src/pages/TopArtists.js",["161","162","163"],"/home/hansen/workspace/spotify-app/client/src/components/Track.js",["164"],"/home/hansen/workspace/spotify-app/client/src/components/TrackList.js",[],"/home/hansen/workspace/spotify-app/client/src/components/Menu.js",[],"/home/hansen/workspace/spotify-app/client/src/components/Artist.js",[],"/home/hansen/workspace/spotify-app/client/src/components/ArtistList.js",[],"/home/hansen/workspace/spotify-app/client/src/components/Profile.js",["165","166","167","168","169","170"],"/home/hansen/workspace/spotify-app/client/src/token.js",[],"/home/hansen/workspace/spotify-app/client/src/context/ToggleContext.js",[],"/home/hansen/workspace/spotify-app/client/src/components/PlaylistItem.js",["171","172","173"],"import React from 'react';\nimport artist from '../images/artist.jpg';\nimport './PlaylistItem.scss';\nimport { Link } from 'react-router-d